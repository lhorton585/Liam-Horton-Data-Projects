      "cell_type": "code",
      "source": [
        "reviews_df.drop(['Unnamed: 0'], axis = 1, inplace = True)"
      ],
      "metadata": {
        "id": "X41sX6Riq1_d"
      },
      "execution_count": null,
      "outputs": []
