      "source": [
        "column_names = ['review_date', 'user_name', 'rating', 'review_text']\n",
        "mustang_harrys_reviews_df = pd.DataFrame(columns=column_names)\n",
        "\n",
        "try:\n",
        "  WebDriverWait(driver, 5).until(EC.visibility_of_element_located((By.XPATH, \"//section[@aria-label='Recommended Reviews']//ul[contains(@class, 'undefined list')]/li\")))\n",
        "except:\n",
        "  pass\n",
        "\n",
        "while True:\n",
        "  html = driver.page_source\n",
        "  respObj = Selector(text=html)\n",
        "\n",
        "  reviews = respObj.xpath(\"//section[@aria-label='Recommended Reviews']//ul[contains(@class, 'undefined list')]/li\")\n",
        "  for review in reviews:\n",
        "    review_date = review.xpath('.//div[contains(@class,\" arrange-unit__09f24__rqHTg arrange-unit-fill__09f24__CUubG\")]/span/text()').extract()[0].strip()\n",
        "    user_name = review.xpath('.//span[contains(@class, \"fs-block css-ux5mu6\")]/a/text()').getall()[0].strip() \n",
        "    rating = review.xpath('.//div[contains(@class, \" five-stars__09f24__mBKym\")]/@aria-label').getall()[0].strip() \n",
        "    review_text = review.xpath(\".//span[contains(@class, ' raw')]/text()\").getall()\n",
        "\n",
        "    mustang_harrys_reviews_df = mustang_harrys_reviews_df.append({'review_date': review_date, 'user_name': user_name , 'rating': rating, 'review_text': review_text}, ignore_index=True)\n",
        "\n",
        "  nextPage = respObj.xpath(\"//a/span[contains(@class, 'chevron-right')]\")\n",
        "  if nextPage:\n",
        "    nextBtnElem = driver.find_element(By.XPATH,\"//a/span[contains(@class, 'chevron-right')]\")\n",
        "    driver.execute_script(\"arguments[0].click()\", nextBtnElem)\n",
        "    time.sleep(2)\n",
        "  else:\n",
        "    break"
      ]
